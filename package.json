{
  "name": "blackbox-ai",
  "version": "0.1.0",
  "description": "Personal AI assistant that records and analyzes your computer activities",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "npm run copy-files && tsc && electron . --dev",
    "compile": "tsc && npm run copy-files",
    "watch": "tsc -w",
    "test": "jest",
    "test:e2e": "npx playwright test",
    "build": "tsc && npm run copy-files && electron-builder",
    "build:win": "tsc && npm run copy-files && electron-builder --win",
    "build:mac": "tsc && npm run copy-files && electron-builder --mac",
    "build:linux": "tsc && npm run copy-files && electron-builder --linux",
    "generate-icons": "electron-icon-builder --input=./assets/icon.png --output=./assets/icons",
    "copy-files": "node scripts/copy-files.js"
  },
  "keywords": [
    "ai",
    "productivity",
    "memory",
    "assistant",
    "recording",
    "electron"
  ],
  "author": "BlackBox AI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/blackbox-ai/blackbox-ai.git"
  },
  "homepage": "https://blackbox-ai.com",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@google/generative-ai": "^0.24.0",
    "@tensorflow/tfjs": "^4.22.0",
    "active-win": "^8.2.1",
    "axios": "^1.8.4",
    "better-sqlite3": "^11.9.1",
    "electron": "^35.2.0",
    "electron-builder": "^26.0.12",
    "electron-store": "^8.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.2",
    "jsdom": "^26.1.0",
    "node-audiorecorder": "^3.0.0",
    "node-microphone": "^0.1.6",
    "node-webpmux": "^3.2.1",
    "openai": "^4.95.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.30.0",
    "screenshot-desktop": "^1.15.1",
    "sequelize": "^6.37.7",
    "sharp": "^0.34.1",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^4.1.4",
    "wav-decoder": "^1.3.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "electron-icon-builder": "^2.0.1",
    "electron-rebuild": "^3.2.9",
    "eslint": "^9.25.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
